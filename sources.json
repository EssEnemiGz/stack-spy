{
  "Wordpress": {
    "html": ["wp-content", "wp-includes"],
    "routes": ["/wp-json/"],
    "headers_patterns": {
      "x-powered-by":"(wp engine)"
    },
    "cookies_patterns": ["wordpress_logged_in"],
    "confidence_weights": {
      "html": 40,
      "routes": 30,
      "headers": 20,
      "cookies": 10
    }
  },
  "Cloudflare": {
    "html": ["https://static.cloudflareinsights.com"],
    "routes": ["/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"],
    "headers_patterns": {
      "Server": "cloudflare",
      "cf-ray": "[A-Za-z0-9]-ATL",
      "cf-cache-status": "(HIT|MISS|DINAMYC)",
      "cf-connecting-ip": "\\d+\\.\\d+\\.\\d+\\.\\d+"
    },
    "cookies_patterns": [],
    "confidence_weights": {
      "html": 10,
      "routes": 20,
      "headers": 70,
      "cookies": 0
    }
  },
  "Shopify": {
    "html": ["cdn.shopify.com", "Shopify.theme", "Shopify.checkout"],
    "routes": ["/collections/", "/products/", "/cart"],
    "headers_patterns": {
      "x-shopify-stage": ".+",
      "x-request-id": ".+"
    },
    "cookies_patterns": ["_shopify_fs", "_shopify_s"],
    "confidence_weights": {
      "html": 30,
      "routes": 30,
      "headers": 30,
      "cookies": 10
    }
  },
  "Joomla": {
    "html": ["content=\"Joomla! - Open Source Content Management\"", "Joomla\\\\CMS"],
    "routes": ["/index.php?option=", "/component/"],
    "headers_patterns": {
      "X-Content-Encoded-By": "Joomla",
      "Set-Cookie": "joomla_[a-zA-Z0-9]+"
    },
    "cookies_patterns": ["joomla_[a-zA-Z0-9]+"],
    "confidence_weights": {
      "html": 40,
      "routes": 30,
      "headers": 20,
      "cookies": 10
    }
  },
  "Wix": {
    "html": ["wix.com", "wixstatic.com", "WixSite"],
    "routes": ["/_wix"], 
    "headers_patterns": {
      "x-wix-request-id": ".+",
      "x-wix-debug": ".+"
    },
    "cookies_patterns": ["wixUID", "wixCIDX", "XSRF-TOKEN"],
    "confidence_weights": {
      "html": 30,
      "routes": 20,
      "headers": 40,
      "cookies": 10
    }
  }
}
